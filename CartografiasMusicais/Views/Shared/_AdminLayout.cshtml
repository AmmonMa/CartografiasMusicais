    <script src="~/admin/js/bootstrap/bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/admin/js/jRespond/jRespond.min.js" asp-append-version="true"></script>
    <script src="~/admin/js/sparkline/jquery.sparkline.min.js" asp-append-version="true"></script>
    <script src="~/admin/js/slimscroll/jquery.slimscroll.min.js" asp-append-version="true"></script>
