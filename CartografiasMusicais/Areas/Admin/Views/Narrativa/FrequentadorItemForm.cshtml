@model CartografiasMusicais.CrossCutting.ValidationModels.Narrativa.NarrativaItemValidationModel


@using HtmlHelpers.BeginCollectionItemCore;
@using (Html.BeginCollectionItem("Frequentadores"))
{
    @Html.HiddenFor(model => model.Id)
    @Html.HiddenFor(model => model.CaminhoImagem)
    <table class="table table-custom">
        <tr>
            <td>
                @Html.EditorFor(model => model.Video, new { htmlAttributes = new { @class = "form-control" } })
            </td>

            <td>
                <button data-action="removeItem" class="btn btn-default">Remover</button>
            </td>
        </tr>
    </table>
}