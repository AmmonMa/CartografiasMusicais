                <div id="sidebar-wrap">
                    <div class="panel-group slim-scroll" role="tablist">
                        <div class="panel panel-default">
                            <div class="panel-heading" role="tab">
                                <h4 class="panel-title">
                                    <a data-toggle="collapse" href="#sidebarNav">
                                        Menu Principal <i class="fa fa-angle-up"></i>
                                    </a>
                                </h4>
                            </div>
                            <div id="sidebarNav" class="panel-collapse collapse in" role="tabpanel">
                                <div class="panel-body">
                                    <ul id="navigation">
                                        <li>
                                            <a href="#"><i class="fa fa-users"></i> <span>Usuários</span></a>
                                            <ul>
                                                <li><a asp-controller="User" asp-action="Index"><i class="fa fa-caret-right"></i> Listar Todos</a></li>
                                                <li><a asp-controller="User" asp-action="Create"><i class="fa fa-caret-right"></i> Cadastrar Usuário</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-sort-amount-desc" name="sort-amount-desc"></i><span>Cidades</span></a>
                                            <ul>
                                                <li><a asp-controller="Cidade" asp-action="Index"><i class="fa fa-caret-right"></i> Listar Todos</a></li>
                                                <li><a asp-controller="Cidade" asp-action="Create"><i class="fa fa-caret-right"></i> Cadastrar Cidade</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-sort-amount-desc" name="sort-amount-desc"></i><span>Musica</span></a>
                                            <ul>
                                                <li><a asp-controller="Musica" asp-action="Index"><i class="fa fa-caret-right"></i> Listar Todos</a></li>
                                                <li><a asp-controller="Musica" asp-action="Create"><i class="fa fa-caret-right"></i> Cadastrar Info</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-sort-amount-desc" name="sort-amount-desc"></i><span>Corpos</span></a>
                                            <ul>
                                                <li><a asp-controller="Corpo" asp-action="Index"><i class="fa fa-caret-right"></i> Listar Todos</a></li>
                                                <li><a asp-controller="Corpo" asp-action="Create"><i class="fa fa-caret-right"></i> Cadastrar Info</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-sort-amount-desc" name="sort-amount-desc"></i><span>Grupos</span></a>
                                            <ul>
                                                <li><a asp-controller="Grupo" asp-action="Index"><i class="fa fa-caret-right"></i> Listar Todos</a></li>
                                                <li><a asp-controller="Grupo" asp-action="Create"><i class="fa fa-caret-right"></i> Cadastrar Info</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-sort-amount-desc" name="sort-amount-desc"></i><span>Espaços</span></a>
                                            <ul>
                                                <li><a asp-controller="Espaco" asp-action="Index"><i class="fa fa-caret-right"></i> Listar Todos</a></li>
                                                <li><a asp-controller="Espaco" asp-action="Create"><i class="fa fa-caret-right"></i> Cadastrar Info</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a href="#"><i class="fa fa-sort-amount-desc" name="sort-amount-desc"></i><span>Narrativas</span></a>
                                            <ul>
                                                <li><a asp-controller="Narrativa" asp-action="Index"><i class="fa fa-caret-right"></i> Listar Todos</a></li>
                                                <li><a asp-controller="Narrativa" asp-action="Create"><i class="fa fa-caret-right"></i> Cadastrar Info</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </aside>
        </div>
